{
    "name": "maddoger/yii2-cms-admin",
    "type": "yii2-extension",
    "description": "Admin panel for Yii2 CMS.",
    "authors": [
        {
            "name": "Vitaliy Syrchikov",
            "email": "maddoger@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "maddoger/yii2-selectize": "*",
        "maddoger/yii2-cms-core": "*",
        "maddoger/yii2-highcharts": "*",
        "bower-asset/font-awesome": "4.*",
        "phpsysinfo/phpsysinfo": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "maddoger\\admin\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}